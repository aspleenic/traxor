<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Event name</th>
      <th>Event city</th>
      <th>Event country</th>
      <th>Event location</th>
      <th>Event total cost</th>
      <th>Event url</th>
      <th>Event tracking url</th>
      <th>Event sponsorship cost</th>
      <th>Event physical goods cost</th>
      <th>Event travel cost</th>
      <th>Event impact score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.event_city %></td>
        <td><%= event.event_country %></td>
        <td><%= event.event_location %></td>
        <td><%= event.event_total_cost %></td>
        <td><%= event.event_url %></td>
        <td><%= event.event_tracking_url %></td>
        <td><%= event.event_sponsorship_cost %></td>
        <td><%= event.event_physical_goods_cost %></td>
        <td><%= event.event_travel_cost %></td>
        <td><%= event.event_impact_score %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
